    "/enterprise/account-analysis": {
      "post": {
        "tags": [
          "data-management"
        ],
        "summary": "Account Analysis API",
        "description": "Analyze usage data for specific users, providing valuable insights into platform usage, design generation, and resource consumption",
        "operationId": "accountAnalysis",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "description": "Analysis parameters",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AccountAnalysisRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "body": {
                      "type": "object",
                      "properties": {
                        "yearlyAnalysis": {
                          "type": "object",
                          "properties": {
                            "numberOfGeneratedDesignVariations": {
                              "type": "integer",
                              "example": 254,
                              "description": "Total number of design variations generated in the past year"
                            },
                            "numberOfEditedDesigns": {
                              "type": "integer",
                              "example": 46,
                              "description": "Total number of designs that were edited"
                            },
                            "numberOfDownloadedDesigns": {
                              "type": "integer",
                              "example": 24,
                              "description": "Total number of designs that were downloaded"
                            },
                            "numberOfMedia": {
                              "type": "integer",
                              "example": 25,
                              "description": "Number of media items uploaded/used"
                            },
                            "mediaSize": {
                              "type": "string",
                              "example": "25Mb",
                              "description": "Total size of media in storage"
                            }
                          }
                        },
                        "monthlyAnalysis": {
                          "type": "array",
                          "description": "Contains monthly breakdown of the same metrics found in YearlyAnalysis",
                          "items": {
                            "type": "object",
                            "properties": {
                              "numberOfGeneratedDesignVariations": {
                                "type": "integer",
                                "example": 54
                              },
                              "numberOfEditedDesigns": {
                                "type": "integer",
                                "example": 16
                              },
                              "numberOfDownloadedDesigns": {
                                "type": "integer",
                                "example": 4
                              },
                              "numberOfMedia": {
                                "type": "integer",
                                "example": 250
                              },
                              "mediaSize": {
                                "type": "string",
                                "example": "2Mb"
                              }
                            }
                          }
                        },
                        "creditBalance": {
                          "type": "object",
                          "properties": {
                            "availableCredits": {
                              "type": "integer",
                              "example": 100,
                              "description": "Number of credits available for the account"
                            },
                            "usedCredits": {
                              "type": "integer",
                              "example": 20,
                              "description": "Number of credits consumed"
                            }
                          }
                        },
                        "lastAccessed": {
                          "type": "integer",
                          "example": 1325312222,
                          "description": "Timestamp of last access"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "User Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 400
                    },
                    "body": {
                      "type": "object",
                      "properties": {
                        "message": {
                          "type": "string",
                          "example": "User does not exist"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Authentication Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthError"
                }
              }
            }
          },
          "422": {
            "description": "Invalid Input",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InputError"
                }
              }
            }
          },
          "500": {
            "description": "Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ServerError"
                }
              }
            }
          }
        }
      }
