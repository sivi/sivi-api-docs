    "/enterprise/extract-brand": {
      "post": {
        "tags": [
          "brand-management"
        ],
        "summary": "Extract Brand",
        "description": "Extract brand information from provided assets",
        "operationId": "extractBrand",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "description": "Brand extraction parameters",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ExtractBrandRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "body": {
                      "type": "object",
                      "properties": {
                        "brandName": {
                          "type": "string",
                          "example": "Sivi",
                          "description": "Extracted name of the brand"
                        },
                        "brandDescription": {
                          "type": "string",
                          "example": "AI design generator for brands and layered vector based design generation",
                          "description": "Extracted description or tagline"
                        },
                        "brandUrl": {
                          "type": "string",
                          "example": "https://sivi.ai",
                          "description": "Original URL provided in the request"
                        },
                        "brandLogo": {
                          "type": "string",
                          "example": "https://sivi.ai/sivi-logo.png",
                          "description": "URL to the extracted logo image"
                        },
                        "brandColors": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "example": ["#5662EC", "#EF9AB2"],
                          "description": "List of extracted brand colors in hex format"
                        },
                        "brandFonts": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          },
                          "example": [],
                          "description": "List of detected fonts (may be empty if fonts cannot be determined)"
                        },
                        "brandPersona": {
                          "type": "object",
                          "description": "Brand persona details",
                          "properties": {
                            "emotions": {
                              "type": "array",
                              "items": {
                                "type": "string"
                              },
                              "example": ["happy"],
                              "description": "List of detected emotions"
                            },
                            "industry": {
                              "type": "string",
                              "example": "games",
                              "description": "Detected industry"
                            },
                            "audience": {
                              "type": "array",
                              "items": {
                                "type": "string"
                              },
                              "example": ["working mom", "working dad"],
                              "description": "List of detected target audience"
                            },
                            "designTags": {
                              "type": "array",
                              "items": {
                                "type": "string"
                              },
                              "example": ["minimal", "time management", "productivity", "health"],
                              "description": "List of detected design tags"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Authentication Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthError"
                }
              }
            }
