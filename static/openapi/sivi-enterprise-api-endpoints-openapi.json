    "/enterprise/export-data": {
      "post": {
        "tags": [
          "data-management"
        ],
        "summary": "Export Data API",
        "description": "Export design variations, media, and content for specific users within a specified date range",
        "operationId": "exportData",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "description": "Export parameters",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ExportDataRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "body": {
                      "type": "object",
                      "properties": {
                        "variants": {
                          "type": "array",
                          "description": "List of design variants created within the date range",
                          "items": {
                            "type": "object",
                            "properties": {
                              "variantImageUrl": {
                                "type": "string",
                                "example": "https://resources.hellosivi.com/user-data/e5ef6aa0-8d6c-11ec-bd33-8d2f1bec7c21/generated/sg0yMSZzNnU--97026520-1f1b-11f0-ada0-01392001fe46--sh0V7MlzOPm.jpg",
                                "description": "URL to the generated design image"
                              },
                              "variantEditLink": {
                                "type": "string",
                                "example": "https://instant.sivi.ai/#/variant/sh0V7MlzOPm/independent-design-editor?type=edited",
                                "description": "Link to edit the design in Sivi's editor"
                              },
                              "variantId": {
                                "type": "string",
                                "example": "sh0V7MlzOPm",
                                "description": "Unique identifier for the variant"
                              }
                            }
                          }
                        },
                        "media": {
                          "type": "array",
                          "description": "List of media assets uploaded/used within the date range",
                          "items": {
                            "type": "object",
                            "properties": {
                              "url": {
                                "type": "string",
                                "example": "https://resources.hellosivi.com/user-data/e5ef6aa0-8d6c-11ec-bd33-8d2f1bec7c21/generated/media-1.jpg",
                                "description": "URL to the media asset"
                              },
                              "type": {
                                "type": "string",
                                "example": "photo",
                                "description": "Type of media (e.g., 'photo', 'illustration')"
                              }
                            }
                          }
                        },
                        "content": {
                          "type": "array",
                          "description": "List of content snippets created within the date range",
                          "items": {
                            "type": "object",
                            "properties": {
                              "title": {
                                "type": "string",
                                "example": "heading",
                                "description": "Content title or heading"
                              },
                              "subtitle": {
                                "type": "string",
                                "example": "subtitle for this snippet",
                                "description": "Content subtitle"
                              },
                              "bulletlist": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                },
                                "example": ["1", "2"],
                                "description": "Array of bullet points"
                              }
                            }
                          }
                        },
                        "next": {
                          "type": "integer",
                          "example": 0,
                          "description": "Indicates if more data is available (0 = no more data, 1 = more data available)"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "User Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 400
                    },
                    "body": {
                      "type": "object",
                      "properties": {
                        "message": {
                          "type": "string",
                          "example": "User does not exist"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Authentication Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthError"
                }
              }
            }
          },
          "422": {
            "description": "Invalid Input",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InputError"
                }
              }
            }
