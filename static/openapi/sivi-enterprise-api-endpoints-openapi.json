    "/enterprise/login-user": {
      "post": {
        "tags": [
          "account-management"
        ],
        "summary": "Login User",
        "description": "Login user with abstract user ID",
        "operationId": "loginUser",
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "requestBody": {
          "description": "User login parameters",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginUserRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "example": 200
                    },
                    "body": {
                      "type": "object",
                      "properties": {
                        "accessToken": {
                          "type": "string",
                          "example": "97026520-1f1b-11f0-ada0-01392001fe46",
                          "description": "JWT token for API authentication, valid for 1 hour"
                        },
                        "refreshToken": {
                          "type": "string",
                          "example": "2342342-1f1b-11f0-ada0-01392001fe46",
                          "description": "Token that can be used to obtain a new access token"
                        },
                        "workspaceId": {
                          "type": "string",
                          "example": "765634-2322-34i44sd3-dkfjgdkjf292",
                          "description": "Unique identifier for the workspace"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Authentication Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthError"
                }
              }
            }
          },
          "422": {
            "description": "Invalid Input",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InputError"
                }
              }
            }
          },
          "500": {
            "description": "Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ServerError"
                }
              }
            }
          }
        }
      }
    },
